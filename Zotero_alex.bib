	keywords = {Automatic control, Control systems, Frequency, Kinetic energy, Laplace equations, Manipulators, Robots, Robust stability, Robustness, Tensile stress, contact tasks, control system, kinematic equations, manipulator dynamics, robot kinematics, stability, unmodeled interface dynamics},
