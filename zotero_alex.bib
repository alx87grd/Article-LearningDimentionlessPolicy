	year = {2014},
	keywords = {Approximation error, Bang-bang control, Force, Manipulators, Motion control, Robots, Switches, Vectors, actuators, binary manipulator, binary robots, cellular redundant actuators, cellular-like redundant binary actuators, electric-gearmotor robotic system, fault-tolerant controller, genetic algorithm, genetic algorithms, influence vector control, pneumatic systems, position control, position control task, probabilistic approach, robust fault-tolerant control scheme, sliding mode approach, soft robots, variable structure systems},
	pages = {642--651},
}

