@misc{lockheed_martin_relief_nodate,
	title = {Relief for the {Daily} {Grind}: {Industrial} {Exoskeletons} at {Work}},
	shorttitle = {Relief for the {Daily} {Grind}},
	url = {http://www.lockheedmartin.com/us/news/features/2014/mfc-103114-relief-daily-grind-industrial-exoskeletons-work.html},
	abstract = {News \& Events      Media Contacts     News Releases     Features         2014 Features         2013 Features         2012 Features     Speeches     Annual Reports     Trade Shows  Relief for the Daily Grind: Industrial Exoskeletons at Work Click to Edit Slides FORTISâ¢ exoskeleton  On reality game shows, pushing contestants to their mental and physical limits is the norm. From scampering through life-size mazes to cracking codes, we revel in the simplicity and difficulty of these tasks. As contestants totter precariously, we find ourselves wondering about the difficulty of their challenge: standing on one foot with arms outstretched for as long as possible.  10 seconds.  30 seconds.  1 minute.  â¦collapsing one after another until the winner remains standing.  For something as seemingly easy as holding your arms outstretched, imagine how much more difficult it would be if in your hands were a 30-pound sandblaster. Thirty pounds doesnât sound like much, but consider that it is roughly the weight of an average toddler, four one-gallon jugs of milk or six laptops. Odds are you wouldnât be able to hold the weight for long before needing to rest.  This is a common scenario in the industrial sector where workers must use heavy, hand-operated tools to complete their tasks. Whether grinding barnacles off a shipâs hull or welding a long section of beam, daily industrial work can be physically exhausting. Thatâs where exoskeleton technology comes in.  âExoskeletons can enhance a personâs physical capabilities,â said Patricia Aelker, program manager of Lockheed Martinâs exoskeleton technologies. âTo increase strength or endurance, you must have a keen understanding about the structure of the human body, the science of how the body moves and the variations among individuals.â  Using that information, as well as lessons from previous proof-of-concept designs, the team developed the FORTISâ¢ exoskeleton, which comfortably accommodates multiple body types while increasing endurance and productivity. FORTISâ¢ exoskeleton Understanding the differences of individual bodies, the FORTIS team designed the exoskeleton to be adjustable to fit a range of body sizes and types.  The FORTIS exoskeleton takes the weight of a load, like a heavy hand tool, off operators, making it easier for them to do their work. Using the principles of biomechanics, the weight is transferred to a mechanical arm and down to the ground through a series of joints, similar to the joints in your hips, knees and ankles.},
	urldate = {2017-11-01},
	author = {Lockheed Martin},
}

@article{altenburger_design_2016,
	title = {Design of a passive, iso-elastic upper limb exoskeleton for gravity compensation},
	volume = {3},
	issn = {2197-4225},
	url = {https://doi.org/10.1186/s40648-016-0051-5},
	doi = {10.1186/s40648-016-0051-5},
	abstract = {An additional mechanical mechanism for a passive parallelogram-based exoskeleton arm-support is presented. It consists of several levers and joints and an attached extension coil spring. The additional mechanism has two favourable features. On the one hand it exhibits an almost iso-elastic behaviour whereby the lifting force of the mechanism is constant for a wide working range. Secondly, the value of the supporting force can be varied by a simple linear movement of a supporting joint. Furthermore a standard tension spring can be used to gain the desired behavior. The additional mechanism is a 4-link mechanism affixed to one end of the spring within the parallelogram arm-support. It has several geometrical parameters which influence the overall behaviour. A standard optimisation routine with constraints on the parameters is used to find an optimal set of geometrical parameters. Based on the optimized geometrical parameters a prototype was constructed and tested. It is a lightweight wearable system, with a weight of 1.9 kg. Detailed experiments reveal a difference between measured and calculated forces. These variations can be explained by a 60Â \% higher pre load force of the tension spring and a geometrical offset in the construction.},
	urldate = {2017-11-01},
	journal = {ROBOMECH Journal},
	author = {Altenburger, Ruprecht and Scherly, Daniel and Stadler, Konrad S.},
	month = apr,
	year = {2016},
	keywords = {Assistive device, Design optimisation, Exoskeleton, Gravity compensation, Iso-elastic, Passive},
	pages = {12},
}

