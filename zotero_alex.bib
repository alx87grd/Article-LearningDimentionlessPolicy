@article{bashford_ownership_2016,
	title = {Ownership and {Agency} of an {Independent} {Supernumerary} {Hand} {Induced} by an {Imitation} {Brain}-{Computer} {Interface}},
	volume = {11},
	issn = {1932-6203},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0156591},
	doi = {10.1371/journal.pone.0156591},
	abstract = {To study body ownership and control, illusions that elicit these feelings in non-body objects are widely used. Classically introduced with the Rubber Hand Illusion, these illusions have been replicated more recently in virtual reality and by using brain-computer interfaces. Traditionally these illusions investigate the replacement of a body part by an artificial counterpart, however as brain-computer interface research develops it offers us the possibility to explore the case where non-body objects are controlled in addition to movements of our own limbs. Therefore we propose a new illusion designed to test the feeling of ownership and control of an independent supernumerary hand. Subjects are under the impression they control a virtual reality hand via a brain-computer interface, but in reality there is no causal connection between brain activity and virtual hand movement but correct movements are observed with 80\% probability. These imitation brain-computer interface trials are interspersed with movements in both the subjectsâ real hands, which are in view throughout the experiment. We show that subjects develop strong feelings of ownership and control over the third hand, despite only receiving visual feedback with no causal link to the actual brain signals. Our illusion is crucially different from previously reported studies as we demonstrate independent ownership and control of the third hand without loss of ownership in the real hands.},
	language = {en},
	number = {6},
	urldate = {2019-02-09},
	journal = {PLOS ONE},
	author = {Bashford, Luke and Mehring, Carsten},
	month = jun,
	year = {2016},
	keywords = {Hands, Man-computer interface, Questionnaires, Rubber, Sense of agency, Skin physiology, Skin temperature, Virtual reality},
	pages = {e0156591},
}

@article{veronneau_high-bandwidth_2018,
	title = {A {High}-{Bandwidth} {Back}-{Drivable} {Hydrostatic} {Power} {Distribution} {System} for {Exoskeletons} {Based} on {Magnetorheological} {Clutches}},
	volume = {3},
	issn = {2377-3766},
	doi = {10.1109/LRA.2018.2812910},
	abstract = {Exoskeletons are increasingly interesting for human assistance applications ranging from rehabilitation to force enhancement. However, today's exoskeletons are relatively slow and lack the mechanical transparency required to complete several daily tasks, mainly due to their bulky and non-back-drivable actuation mechanisms. To improve upon conventional exoskeleton designs, this letter presents a novel power-distribution system that combines magnetorheological (MR) clutches and low-friction hydrostatic transmissions using rolling diaphragms. In such a system, MR clutches are used to rapidly modulate the torque provided from a centralized power source and distribute it to each joint through a high-bandwidth, back-drivable, and low-inertia transmission. The main objective of this letter is to investigate the transparency performance of the MR-hydrostatic power distribution in terms of its force-bandwidth and back-drivability with the aim of being used in future exoskeletons. Experiments with a custom one degree-of-freedom haptic joint are supported by an analytical model that demonstrates the high bandwidth ({\textgreater}40 Hz) and good backdrivability (2-11\% of peak force) of an MR-hydrostatic system.},
	number = {3},
	journal = {IEEE Robotics and Automation Letters},
	author = {VÃ©ronneau, C. and BiguÃ©, J. Lucking and Lussier-Desbiens, A. and Plante, J.},
	month = jul,
	year = {2018},
	keywords = {Bandwidth, Exoskeletons, Force, Friction, Hydraulic systems, MR clutches, MR-hydrostatic power distribution, MR-hydrostatic system, Power distribution, Torque, actuators, back-drivability, back-drivable hydrostatic power distribution system, backdrivability, centralized power source, clutches, diaphragms, force-bandwidth, friction, high-bandwidth, human assistance applications, hydrostatic transmission, hydrostatics, low-friction hydrostatic transmissions, low-inertia transmission, magneto-rheological clutch, magnetorheological clutches, magnetorheology, mechanical transparency, peak force, power transmission (mechanical), rolling diaphragm, transparency, transparency performance},
	pages = {2592--2599},
}

