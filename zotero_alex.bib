	month = apr,
	year = {2004},
	keywords = {Safety, actuation, control, human-friendly, robotics},
	pages = {379--398},
}

@inproceedings{seok_actuator_2012,
	title = {Actuator design for high force proprioceptive control in fast legged locomotion},
	doi = {10.1109/IROS.2012.6386252},
	abstract = {High speed legged locomotion involves high acceleration and extensive loadings of the leg, which impose critical challenges in actuator design. We introduce actuator dimensional analysis for maximizing torque density and transmission `transparency'. A front leg prototype developed based on insight from the analysis is evaluated for direct proprioceptive force control without force sensors. The vertical stiffness controlled leg was tested on a material testing device to calibrate the mechanical impedance of the leg. By compensating transmission impedance from commanded torque, the leg was able to estimate impact force. For the impact test, the mean absolute error as a ratio of full scale sensor force is 0.041 in the 3406 N/m stiffness experiment and is 0.049 in the 5038 N/m experiment. The results indicate that prescribed force profile control is possible during high speed locomotion.},
	booktitle = {2012 {IEEE}/{RSJ} {International} {Conference} on {Intelligent} {Robots} and {Systems} ({IROS})},
	author = {Seok, Sangok and Wang, A and Otten, D. and Kim, Sangbae},
	month = oct,
	year = {2012},
	keywords = {Brushless motors, Force, Gears, Impedance, Torque, actuator design, actuator dimensional analysis, actuators, commanded torque, critical challenges, direct proprioceptive force control, fast legged locomotion, force control, force profile control, full scale sensor force, high acceleration, high force proprioceptive control, high speed legged locomotion, high speed locomotion, impact (mechanical), impact force, legged locomotion, material testing device, materials testing, mean absolute error, mechanical impedance, mechanoception, stiffness experiment, torque density, transmission impedance},
	pages = {1970--1975},
}

@article{hogan_stability_1988,
	title = {On the stability of manipulators performing contact tasks},
	volume = {4},
	issn = {0882-4967},
	doi = {10.1109/56.9305},
	abstract = {Manipulation requires contact with the object being manipulated, and the full potential of robots can only be realized when they are applied to contact tasks. One of the difficulties engendered by contact tasks is that they require intimate dynamic interaction between the robot and its environment. That interaction changes the performance of the robot and can jeopardize the stability of its control system. A discussion is presented of the problem of preserving the stability of a manipulator's control system during contact tasks. It will be shown that contact stability may be guaranteed if the control system provides the manipulator with an appropriately structured dynamic response to environmental inputs. Two aspects of one implementation of such a controller will be considered. Robustness to large errors in the manipulator kinematic equations and to unmodeled interface dynamics is shown},
	number = {6},
	journal = {IEEE Journal of Robotics and Automation},
	author = {Hogan, N.},
	month = dec,
