@inproceedings{duval_flexsea-execute:_2016,
	title = {{FlexSEA}-{Execute}: {Advanced} motion controller for wearable robotic applications},
	shorttitle = {{FlexSEA}-{Execute}},
	doi = {10.1109/BIOROB.2016.7523771},
	abstract = {Wearable robots, such as powered prostheses and active exoskeletons, often rely on electric motors for actuation. Emulating biological joint angle and torque profiles requires special sensors, high peak power and advanced real-time controls. Safety is paramount and fail-safe circuits are required to detect and correct problematic situations. A smaller, lighter circuit can lead to a more efficient and affordable robot. Few commercial motor drivers accommodate all of these requirements. In this paper we present FlexSEA-Execute (Figure 1), the advanced motion controller part of FlexSEA, the FLEXible, Scalable Electronics Architecture designed for wearable robotic applications. At 36cm3 and 34.8g, this PSoC-based design integrates a 8A/25A (continuous/pulse) brushed/brushless motor driver, a safety co-processor, multi-drop RS-485, a strain gauge amplifier, a 6-axis inertial motion unit (IMU), USB, and a programmable expansion connector.},
	booktitle = {{IEEE} {International} {Conference} on {Biomedical} {Robotics} and {Biomechatronics} ({BioRob})},
