	keywords = {Couplings, DLR hand-arm system, Dynamics, Humans, Joints, Robots, Torque, biological systems, elasticity, link side kinetic energy, manipulator dynamics, optimal control, potential energy, robot design, robot dynamics, variable stiffness arms, variable stiffness robots},
	pages = {3347--3354},
}

