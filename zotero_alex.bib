}

@article{zhang_finite_2005,
	title = {Finite element simulation of tire-snow interaction under combined longitudinal and lateral slip condition},
	abstract = {Tire-snow interaction is studied using the general purpose finite element (FE) program ABAQUS/Explicit. The tire sinkage, snow sinkage, tire deflection, motion resistance and traction are obtained from the FE simulation. The FE simulation shows that the response of a tire rolling on snow depends on snow sinkage, snow shear deformation, and the properties of contact interface. The rotational torque and drawbar pull at the tire center and the traction and motion resistance on the contact patch are studied with slips and normal load as inputs. The relationships between all these interfacial forces and the inputs (longitudinal slip, slip angle and vertical wheel load) are presented. It is found that the relation between longitudinal traction and lateral traction follows a "conceptual" friction ellipse with distinctive characteristics.},
	journal = {15th International Conference of the International Society for Terrain Vehicle Systems 2005, ISTVS 2005},
	author = {Zhang, T. and Lee, J.H. and Liu, Q.},
	month = jan,
	year = {2005},
