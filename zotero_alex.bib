@article{li_linearized_2015,
	title = {Linearized {Torque} {Actuation} {Using} {FPGA}-{Controlled} {Magnetorheological} {Actuators}},
	volume = {20},
	issn = {1083-4435},
	doi = {10.1109/TMECH.2014.2317714},
	abstract = {In recent years, magnetorheological (MR) clutches have been increasingly used for realizing compliant actuation. One difficulty in using MR clutches is the existence of nonlinear hysteretic behaviors between the input current and output torque of an MR clutch. In this paper, a new closed-loop, field-programable-gate-array (FPGA)-based control scheme to linearize an MR clutch's input-output relationship is presented. The feedback signal used in this control scheme is the magnetic field acquired from hall sensors within the MR clutch. The FPGA board uses this feedback signal to compensate for the nonlinear behavior of the MR clutch using an estimated model of the clutch magnetic field. The local use of an FPGA board will dramatically simplify the use of MR clutches for torque actuation. The effectiveness of the proposed technique is validated using an experimental platform that includes an MR clutch as part of a compliant actuation mechanism. The results clearly demonstrate that the use of the proposed FPGA-based closed-loop control scheme can effectively eliminate hysteretic behaviors of the MR clutch, allowing to have linear actuators with predictable behaviors.},
	number = {2},
	journal = {IEEE/ASME Transactions on Mechatronics},
	author = {Li, W. and Yadmellat, P. and Kermani, M. R.},
	month = apr,
	year = {2015},
	keywords = {FPGA board, FPGA-based closed loop control scheme, Field-programmable gate array (FPGA), Filed-programmable gate array (FPGA), Hall sensor, MR clutch input-output relationship, MR clutches, Magnetic hysteresis, Magnetic sensors, Magnetomechanical effects, Stress, Torque, clutch magnetic field, clutches, compliant actuation mechanism, feedback signal, field programable gate array, field programmable gate arrays, hall sensors, linear actuators, linearized torque actuation, magnetorheological actuator, magnetorheological actuators, magnetorheological clutches, magnetorheology, nonlinear behavior, nonlinear hysteretic behaviors, predictable behaviors, torque control},
